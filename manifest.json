{
    "name": "Remove Stupid",

    "description": "Filter out paragraphs which contain an interval, followed by punctuation.",
    "version": "0.1",
    "manifest_version": 2,

    "browser_action": {
        "default_popup": "options.html"
    },

    "content_scripts": [{
        "js": ["filter.js"],
        "matches": ["<all_urls>"]
    }],

    "background": {
      "service_worker": "init.js"
    },

    "permissions": [
        "storage"
    ]
}